import java.util.Locale;
import java.util.Scanner;

public class Main {
	
	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		double pi, raio, saida;
		
		pi = 3.14159;
		raio = sc.nextDouble();
		saida = pi * (raio ** 2);
		
		sc.close();
		
	}

}
