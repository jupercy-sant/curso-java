import java.util.Locale;
import java.util.Scanner;

public class Main {

	public static void main(String[] args) {
		
		Locale.setDefault(Locale.US);
		Scanner sc = new Scanner(System.in);
		
		int pecaA, pecaB, numeroDePecasA, numeroDePecasB;
		double valorA, valorB, totalA, totalB;
		
		pecaA = sc.nextInt();
		numeroDePecasA = sc.nextInt();
		valorA = sc.nextDouble();
		
		pecaB = sc.nextInt();
		numeroDePecasB = sc.nextInt();
		valorB = sc.nextDouble();
		
		totalA = numeroDePecasA * valorA;
		totalB = numeroDePecasB * valorB;
		
		System.out.printf("VALOR A PAGAR: R$ %.2f%n", (totalA + totalB));
		
		sc.close();
	}

}
